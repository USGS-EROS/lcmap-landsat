{
  :database  {:cluster {:contact-points "localhost"
                        :socket-options {:read-timeout-millis 20000}}
              :default-keyspace "lcmap_landsat_dev"}
  :event     {
              :host "localhost"
              :port 5672
              :queues [{:name "dev.lcmap.landsat.server.queue"
                        :opts {:durable true
                               :exclusive false
                               :auto-delete false}}
                       {:name "dev.lcmap.landsat.worker.queue"
                        :opts {:durable true
                               :exclusive false
                               :auto-delete false}}]
              :exchanges [{:name "dev.lcmap.landsat.server.exchange"
                           :type "topic"
                           :opts {:durable true}}
                          {:name "dev.lcmap.landsat.worker.exchange"
                           :type "topic"
                           :opts {:durable true}}]
              :bindings [{:exchange "dev.lcmap.landsat.server.exchange"
                          :queue "dev.lcmap.landsat.worker.queue"
                          :opts {:routing-key "ingest"}}]}
 :http      {:port 5678
             :join? false
             :daemon? true}
 :server    {:exchange "dev.lcmap.landsat.server.exchange"
             :queue "dev.lcmap.landsat.server.queue"}
 :worker    {:exchange "dev.lcmap.landsat.worker.exchange"
             :queue    "dev.lcmap.landsat.worker.queue"}
 :search     {:index-url      "http://localhost:9200/tile-specs"
              :refresh-url    "http://localhost:9200/tile-specs/_refresh"
              :bulk-api-url   "http://localhost:9200/tile-specs/dev/_bulk"
              :search-api-url "http://localhost:9200/tile-specs/_search"
              :max-result-size 10000}}
